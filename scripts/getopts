#!/bin/bash

# pass arguments from cli
while getopts a:c:s:p:h:m: option
do
    case "${option}"
        in
        a) atac=${OPTARG};;
        c) cite=${OPTARG};;
        s) samples=${OPTARG};;
        p) params=${OPTARG};;
        h) harmonize=${OPTARG};;
        m) mixscape=${OPTARG};;
    esac
done

